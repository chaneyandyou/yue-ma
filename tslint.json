{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts"
    ]
  },
  "rules": {
    "no-console": [
      false
    ],
    "ordered-imports": [
      false
    ],
    "semicolon": [
      true
    ],
    "object-literal-sort-keys": [
      false
    ]
  }
}
